#ifndef USER_H
#define USER_H

#include <string>
iostream

// Класс пользователя
template <typename T>
class User {
public:
    string login;
    string password;
    string name;

    User(const string& login, const string& password, const string& name)
        : login(login), password(password), name(name) {}
};

#endif // USER_H
